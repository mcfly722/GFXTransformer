<?xml version="1.0" ?>
<!-- Configuration of the Ant build system to generate a Jar file -->
<project name="GFXTransformer" default="CreateJar">
	<target name="CreateJar" description="Create Jar file">
		<jar jarfile="GFXTransformer.jar" basedir="bin" includes="**">
			<manifest>
				<attribute name="Main-Class" value="GFXTransformer.Main" />
			</manifest>
		</jar>
	</target>
</project>